<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>No</th>
          <th>Product</th>
          <th>Order</th>
        </tr>
      </thead>
      <tbody>
        <h3>Report</h3>
        <% let no = 0;%>
        <% data.forEach((rep) => { %>
          <% no++; %>
          <tr>
            <td><%= no %></td>
            <td><%= rep.product %></td>
            <td><%= rep.order %></td>
          </tr>
        <% }) %>
      </tbody>
    </table
  </body>
  <a href="/">Back</a>
</html>
