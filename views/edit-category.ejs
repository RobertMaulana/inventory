<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Update Category</title>
  </head>
  <body>
    <% data.forEach((category) => { %>
      <h3>Category Update (ID: <%= category.id%>)</h3>
      <form action="/update-category" method="post">
        <input type="hidden" name="id" value="<%= category.id %>">
        <label for="nama_category">Nama Category</label><br>
        <input type="text" name="nama_category" value="<%= category.nama_category%>"><br>
        <button type="submit" name="button">Submit Update</button>
      </form>
    <% }) %>
  </body>
</html>
